<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>5</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>360</left>
		<top>90</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="group">
			<include name="DialogBackgroundCommons">
				<param name="DialogBackgroundWidth" value="1200" />
				<param name="DialogBackgroundHeight" value="900" />
				<param name="DialogHeaderLabel" value="-" />
				<param name="DialogHeaderId" value="2" />
			</include>
			<control type="label" id="4">
				<description>No Settings Label</description>
				<left>30</left>
				<top>270</top>
				<width>1140</width>
				<align>center</align>
				<label>13389</label>
				<font>font15</font>
			</control>
			<control type="list" id="5">
				<left>32</left>
				<top>88</top>
				<width>1138</width>
				<height>780</height>
				<onleft>60</onleft>
				<onright>60</onright>
				<onup>5</onup>
				<ondown>5</ondown>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<itemlayout height="60" width="1138">
					<control type="label">
						<width>1138</width>
						<height>60</height>
						<textoffsetx>15</textoffsetx>
						<font>font14</font>
						<textcolor>maintext</textcolor>
						<selectedcolor>$VAR[ColorHighlightVar]</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<width>1138</width>
						<height>60</height>
						<textoffsetx>15</textoffsetx>
						<font>font14</font>
						<textcolor>maintext</textcolor>
						<selectedcolor>$VAR[ColorHighlightVar]</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</itemlayout>
				<focusedlayout height="60" width="1138">
					<control type="image">
						<width>1138</width>
						<height>60</height>
						<texture>listselect_fo.png</texture>
						<colordiffuse>$VAR[ColorHighlightVar]</colordiffuse>
						<visible>Control.HasFocus(5)</visible>
						<include>VisibleFadeEffect</include>
					</control>
					<control type="label">
						<width>1138</width>
						<height>60</height>
						<textoffsetx>15</textoffsetx>
						<font>font14</font>
						<textcolor>maintext</textcolor>
						<selectedcolor>$VAR[ColorHighlightVar]</selectedcolor>
						<scroll>true</scroll>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
						<animation effect="fade" start="100" end="70" time="300" tween="sine" easing="inout" condition="!Control.HasFocus(5)">Conditional</animation>
					</control>
					<control type="label">
						<width>1138</width>
						<height>60</height>
						<textoffsetx>15</textoffsetx>
						<font>font14</font>
						<textcolor>maintext</textcolor>
						<selectedcolor>$VAR[ColorHighlightVar]</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
						<animation effect="fade" start="100" end="70" time="300" tween="sine" easing="inout" condition="!Control.HasFocus(5)">Conditional</animation>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>1172</left>
				<top>88</top>
				<height>780</height>
				<width>8</width>
				<onleft>5</onleft>
				<onright>5</onright>
				<ondown>60</ondown>
				<onup>60</onup>
			</control>
		</control>
	</controls>
</window>